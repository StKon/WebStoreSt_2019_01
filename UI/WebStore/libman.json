{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@2.2.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@3.3.7",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/jquery-ajax-unobtrusive",
      "files": [
        "dist/jquery.unobtrusive-ajax.js",
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_ru.js",
        "localization/messages_ru.min.js"
      ]
    },
    {
      "library": "jquery-loading-overlay@2.1.6",
      "destination": "wwwroot/jquery-loading-overlay/"
    }
  ]
}